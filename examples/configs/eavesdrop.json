{
	"root": ".",
	"tmp": "tmp",
	"build": "go build -o ./tmp/main ./main.go",
	"run": "tmp/main",
	"ignore_dirs": [
		"assets",
		"data",
		"node_modules",
		"testdata",
		"tmp",
		"vendor"
	],
	"ignore_files": [],
	"ignore_regex": [
		"^\\.{1}.*$",
		"^.*_templ\\.go$",
		"^.*_test\\.go$"
	],
	"watch_files": [],
	"watch_exts": [
		".go",
		".html",
		".templ",
		".tmpl",
		".tpl"
	],
	"proxy": false,
	"app_port": 8000,
	"proxy_port": 8001
}
